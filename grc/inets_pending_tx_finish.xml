<?xml version="1.0"?>
<block>
  <name>pending_tx_finish</name>
  <key>inets_pending_tx_finish</key>
  <category>inets_dmf_phy</category>
  <import>import inets</import>
  <make>inets.pending_tx_finish($develop_mode, $block_id, $system_time_granularity_us, $sample_rate, $lengthtagname)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Develop mode</name>
    <key>develop_mode</key>
    <value>develop_mode</value>
    <type>int</type>
    <option>
      <name>on</name>
      <key>1</key>
    </option>
    <option>
      <name>off</name>
      <key>0</key>
    </option>
    <option>
      <name>time</name>
      <key>2</key>
    </option>
  </param>
  <param>
    <name>Block id</name>
    <key>block_id</key>
    <value>block_id</value>
    <type>int</type>
  </param>
  <param>
    <name>System time granularity [us]</name>
    <key>system_time_granularity_us</key>
    <value>system_time_granularity_us</value>
    <type>int</type>
  </param>
  <param>
    <name>sample rate</name>
    <key>sample_rate</key>
    <value>samp_rate</value>
    <type>float</type>
  </param>
  <param>
    <name>Length tag name</name>
    <key>lengthtagname</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>tx_frame_info_in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>data_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>ack_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>beacon_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>rts_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>cts_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>ampdu_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>amsdu_frame_out</name>
    <type>message</type>
  </source>
  <source>
    <name>unknown_frame_out</name>
    <type>message</type>
  </source>
</block>
