<?xml version="1.0"?>
<block>
  <name>frame_type_check</name>
  <key>inets_frame_type_check</key>
  <category>inets_dmf_mac</category>
  <import>import inets</import>
  <make>inets.frame_type_check($data_frame, $ack_frame, $beacon_frame, $rts_frame, $cts_frame, $ampdu_frame, $amsdu_frame, $other_frame)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <param>
    <name>Data frame</name>
    <key>data_frame</key>
    <value>0</value>
    <type>enum</type>
    <hide>$data_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>
  <param>
    <name>ACK frame</name>
    <key>ack_frame</key>
    <value>0</value>
    <type>enum</type>
    <hide>$ack_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>
  <param>
    <name>Beacon frame</name>
    <key>beacon_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$beacon_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>
  <param>
    <name>RTS frame</name>
    <key>rts_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$rts_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>
  <param>
    <name>CTS frame</name>
    <key>cts_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$cts_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>

  <param>
    <name>AMPDU frame</name>
    <key>ampdu_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$ampdu_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>

  <param>
    <name>AMSDU frame</name>
    <key>amsdu_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$amsdu_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>
  
  <param>
    <name>Other frame</name>
    <key>other_frame</key>
    <value>1</value>
    <type>enum</type>
    <hide>$other_frame.hide</hide>
    <option>
      <name>Check</name>
      <key>0</key>
      <opt>hide:none</opt>
    </option>
    <option>
      <name>Not check</name>
      <key>1</key>
      <opt>hide:part</opt>
    </option>
  </param>

  <sink>
    <name>frame_info_in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>data_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$data_frame</hide>
  </source>
  <source>
    <name>ack_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$ack_frame</hide>
  </source>
  <source>
    <name>beacon_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$beacon_frame</hide>
  </source>
  <source>
    <name>rts_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$rts_frame</hide>
  </source>
  <source>
    <name>ampdu_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$ampdu_frame</hide>
  </source>
  <source>
    <name>amsdu_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$amsdu_frame</hide>
  </source>
  <source>
    <name>other_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$other_frame</hide>
  </source>
  <source>
    <name>cts_frame_info_out</name>
    <type>message</type>
    <optional>1</optional>
    <hide>$cts_frame</hide>
  </source>
</block>
