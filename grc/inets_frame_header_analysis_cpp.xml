<?xml version="1.0"?>
<block>
  <name>frame_header_analysis_cpp</name>
  <key>inets_frame_header_analysis_cpp</key>
  <category>inets_dmf_v1.0</category>
  <import>import inets</import>
  <make>inets.frame_header_analysis_cpp($develop_mode, $len_frame_type, $len_frame_index, $len_destination_address, $len_source_address, $len_reserved_field_I, $len_reserved_field_II, $len_payload_length)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->

  <param>
    <name>Develop mode</name>
    <key>develop_mode</key>
    <value>develop_mode</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of frame type</name>
    <key>len_frame_type</key>
    <value>len_frame_type</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of frame index</name>
    <key>len_frame_index</key>
    <value>len_frame_index</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of destination address</name>
    <key>len_destination_address</key>
    <value>len_destination_address</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of source address</name>
    <key>len_source_address</key>
    <value>len_source_address</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of reserved field I</name>
    <key>len_reserved_field_I</key>
    <value>len_reserved_field_I</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of reserved field II</name>
    <key>len_reserved_field_II</key>
    <value>len_reserved_field_II</value>
    <type>int</type>
  </param>
  <param>
    <name>Length of payload length field</name>
    <key>len_payload_length</key>
    <value>len_payload_length</value>
    <type>int</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>frame_in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>frame_type_out</name>
    <type>message</type>
  </source>

  <source>
    <name>frame_index_out</name>
    <type>message</type>
  </source>

  <source>
    <name>destination_address_out</name>
    <type>message</type>
  </source>

  <source>
    <name>source_address_out</name>
    <type>message</type>
  </source>

  <source>
    <name>payload_length_out</name>
    <type>message</type>
  </source>

  <source>
    <name>frame_header_out</name>
    <type>message</type>
  </source>
  <source>

    <name>frame_out</name>
    <type>message</type>
  </source>
</block>
